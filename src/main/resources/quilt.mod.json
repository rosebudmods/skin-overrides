{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${group}",
        "id": "skin_overrides",
        "version": "${version}",
        "metadata": {
            "name": "skin overrides",
            "description": "a powerful mod for changing skins and capes!",
            "contributors": {
                "rosebud": "owner",
                "orifu": "creator"
            },
            "contact": {
                "homepage": "https://rosebud.dev/skin-overrides/",
                "sources": "https://lumity.dev/orifu/skin-overrides",
                "issues": "https://github.com/rosebudmods/skin-overrides/issues"
            },
            "license": "MPL-2.0",
            "icon": "assets/skin_overrides/icon.png"
        },
        "intermediate_mappings": "net.fabricmc:intermediary",
        "entrypoints": {
            "main": "net.orifu.skin_overrides.ModInit",
            "client": "net.orifu.skin_overrides.ModClient",
            "${modmenu_entrypoint}": "net.orifu.skin_overrides.ModMenuEntrypoint"
        },
        "depends": [
            {
                "id": "quilt_loader",
                "versions": ">=0.19.1"
            },
            {
                "id": "minecraft",
                "versions": "^${minecraft_version}"
            },
            {
                "id": "fabric"
            },
            {
                "id": "modmenu",
                "optional": true
            }
        ]
    },
    "mixin": "${mixin_file}",
    "access_widener": "aw/${access_widener}.accesswidener"
}
