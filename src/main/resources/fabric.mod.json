{
    "schemaVersion": 1,
    "id": "skin_overrides",
    "version": "${version}",
    "name": "skin overrides",
    "description": "a powerful mod for changing skins and capes!",
    "authors": ["rosebud", "orifu"],
    "contact": {
        "homepage": "https://rosebud.dev/skin-overrides/",
        "sources": "https://lumity.dev/orifu/skin-overrides",
        "issues": "https://github.com/rosebudmods/skin-overrides/issues"
    },
    "license": "MPL-2.0",
    "icon": "assets/skin_overrides/icon.png",
    "entrypoints": {
        "main": ["net.orifu.skin_overrides.ModInit"],
        "client": ["net.orifu.skin_overrides.ModClient"],
        "${modmenu_entrypoint}": ["net.orifu.skin_overrides.ModMenuEntrypoint"]
    },
    "depends": {
        "fabricloader": ">=0.15.11",
        "minecraft": "^${minecraft_version}",
        "fabric": "*"
    },
    "suggests": {
        "modmenu": "*"
    },
    "mixins": ["${mixin_file}"],
    "accessWidener": "aw/${access_widener}.accesswidener"
}
